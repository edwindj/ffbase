\name{mean.ffdf}
\alias{mean.ffdf}
\title{Mean of ffdf vector}
\usage{
  \method{mean}{ffdf} (x, ..., range = NULL)
}
\arguments{
  \item{x}{a \code{ffdf}}

  \item{...}{other arguments passed to
  \code{\link{mean.ff}}}

  \item{range}{a \code{ri} or an \code{integer} vector of
  \code{length==2} giving a range restriction for chunked
  processing}
}
\value{
  a vector with the mean values
}
\description{
  Mean of ffdf vector
}

